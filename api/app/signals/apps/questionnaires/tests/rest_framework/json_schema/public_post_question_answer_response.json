{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"_links": {
			"type": "object"
		},
		"_display": {
			"type": "string"
		},
		"payload": {
			"anyOf": [
				{
					"type": "string"
				},
				{
					"type": "null"
				},
				{
					"type": "object"
				}
			]
		},
		"session": {
			"anyOf": [
				{
					"type": "string"
				},
				{
					"type": "null"
				}
			]
		},
		"created_at": {
			"anyOf": [
				{
					"type": "string"
				},
				{
					"type": "null"
				}
			]
		},
		"next_question": {
			"anyOf": [
				{
					"type": "object",
					"properties": {
						"_links": {
							"type": "object",
							"properties": {
								"self": {
									"type": "object",
									"properties": {
										"href": {
											"type": "string"
										}
									},
									"required": [
										"href"
									]
								},
								"sia:post-answer": {
									"type": "object",
									"properties": {
										"href": {
											"type": "string"
										}
									},
									"required": [
										"href"
									]
								}
							},
							"required": [
								"self",
								"sia:post-answer"
							]
						},
						"_display": {
							"type": "string"
						},
						"uuid": {
							"type": "string"
						},
						"label": {
							"oneOf": [
								{"type": "string"},
								{"type": "null"}
							]						},
						"short_label": {
							"oneOf": [
								{"type": "string"},
								{"type": "null"}
							]						},
						"field_type": {
							"type": "string"
						},
						"next_rules": {
							"anyOf": [
								{
									"type": "null"
								},
								{
									"type": "array"
								},
								{
									"type": "object"
								}
							]
						},
						"required": {
							"type": "boolean"
						}
					},
					"required": [
						"_links",
						"_display",
						"uuid",
						"label",
						"short_label",
						"field_type",
						"next_rules",
						"required"
					]
				},
				{
					"type": "null"
				}
			]
		}
	},
	"required": [
		"_links",
		"_display",
		"payload",
		"session",
		"created_at"
	]
}
